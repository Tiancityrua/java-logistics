<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="freight.dao.invoice.in_invoice_dao">
    <insert id="in_delivery" parameterType="freight.DO.Delivery">
        INSERT IGNORE INTO deliverynote VALUES (#{hawb},#{date},#{pieces},#{weight},#{deliveryAdd})
    </insert>
    <insert id="in_duty" parameterType="freight.DO.Invoice">
        INSERT IGNORE INTO duty_invoice VALUES (#{invoiceNo},#{towho},#{invoiceDate},#{billLaden},#{origin},#{dstn},#{natureQuantity},#{duty},#{vat},#{all})
    </insert>
    <insert id="in_freight" parameterType="freight.DO.Invoice">
        INSERT IGNORE INTO freight_invoice VALUES (#{invoiceNo},#{towho},#{invoiceDate},#{billLaden},#{origin},#{dstn},#{natureQuantity},#{freight},#{all})
    </insert>
    <insert id="in_log" parameterType="freight.DO.Invoice">
        INSERT IGNORE INTO logistics_invoice VALUES (#{invoiceNo},#{towho},#{invoiceDate},#{billLaden},#{origin},#{dstn},#{natureQuantity},#{custom},#{handling},#{delivery},#{profit},#{all})
    </insert>
</mapper>